<wpml-config>
    <custom-fields>
        <custom-field action="translate">address</custom-field>
        <custom-field action="translate">col-subtitle</custom-field>
        <custom-field action="translate">col-title</custom-field>
        <custom-field action="translate">map</custom-field>
        <custom-field action="translate">pkb_author_name</custom-field>
        <custom-field action="translate">pkb_author_title</custom-field>
        <custom-field action="ignore">pkb_cf7_id</custom-field>
        <custom-field action="ignore">pkb_cf7_info</custom-field>
        <custom-field action="translate">pkb_contact_info</custom-field>
        <custom-field action="translate">pkb_contact_info_title</custom-field>
        <custom-field action="ignore">pkb_galleryr_bg_color</custom-field>
        <custom-field action="ignore">pkb_gallery_bg_color</custom-field>
        <custom-field action="ignore">pkb_gallery_caption_color</custom-field>
        <custom-field action="ignore">pkb_gallery_excerpt_color</custom-field>
        <custom-field action="translate">pkb_image_caption</custom-field>
        <custom-field action="ignore">pkb_image_url</custom-field>
        <custom-field action="ignore">pkb_map_code</custom-field>
        <custom-field action="translate">pkb_slider_bg_color</custom-field>
        <custom-field action="ignore">pkb_slide_caption_color</custom-field>
        <custom-field action="ignore">pkb_slide_caption_left</custom-field>
        <custom-field action="ignore">pkb_slide_caption_top</custom-field>
        <custom-field action="translate">pkb_slide_cta</custom-field>
        <custom-field action="translate">pkb_slide_desc</custom-field>
        <custom-field action="ignore">pkb_slide_excerpt_color</custom-field>
        <custom-field action="translate">pkb_testimonial_content</custom-field>
        <custom-field action="translate">pkb_text_taxonomy_radio</custom-field>
        <custom-field action="translate">pkb_text_taxonomy_select</custom-field>
        <custom-field action="translate">pkb_upload_image</custom-field>
        <custom-field action="translate">pkb_upload_image2</custom-field>
        <custom-field action="translate">thumbnail</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">gallery</custom-type>
        <custom-type translate="1">nav_menu_item</custom-type>
        <custom-type translate="1">slide</custom-type>
        <custom-type translate="1">testimonial</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="1">media-type</taxonomy>
    </taxonomies>
    <admin-texts>
        <key name="peekaboo_options">
            <key name="pkb_credit" />
            <key name="pkb_quickmenu_title" />
            <key name="pkb_headline_title" />
            <key name="pkb_headline_content" />
            <key name="pkb_headline_button" />
            <key name="pkb_more_link_home" />
            <key name="pkb_more_link" />
            <key name="pkb_custom_404_title" />
            <key name="pkb_custom_404_msg" />
            <key name="pkb_msg_success" />
            <key name="pkb_msg_fail" />
            <key name="pkb_custom_search_title" />
            <key name="pkb_custom_search_msg" />
        </key>
    </admin-texts>
</wpml-config>





